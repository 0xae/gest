<form ng-submit="sendAcomp()" name="acompForm">
    <div class="form-group">
        <label>Data de envio</label>
        <input class="form-control" 
            type="text"
            required="required"
            ng-model="req.sendDate"
        />
    </div>

    <div class="form-group">
        <label>Autoridade enviado</label>
        <input class="form-control" 
            required="required"
            ng-model="req.sendTo"
        />
    </div>

    <button type="submit" class="btn btn-success">Enviar formul&aacute;rio</button>
</form>
