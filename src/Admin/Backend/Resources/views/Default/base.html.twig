<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
        <title>
          {% block title %}Bem vindo!{% endblock %}
        </title>

        {% stylesheets
            '@BackendBundle/Resources/public/css/bootstrap.min.css'
            '@BackendBundle/Resources/public/css/font-awesome.min.css'
            '@BackendBundle/Resources/public/css/ionicons.min.css'
            '@BackendBundle/Resources/public/css/jquery-jvectormap-1.2.2.css'
            '@BackendBundle/Resources/public/css/AdminLTE.min.css'
            '@BackendBundle/Resources/public/css/blue.css'
            '@BackendBundle/Resources/public/css/pace.min.css'
            '@BackendBundle/Resources/public/css/_all-skins.min.css'
            '@BackendBundle/Resources/public/css/app.css'
            '@BackendBundle/Resources/public/lib/editor/trumbowyg.min.css'
        %}
            <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <style>
            h3.app-title {
                margin: 0px;
                margin-top: 15px;
                text-align: center;
                font-size: 17px;
                font-weight: bold;
            }
            .bg-arfa {
                background-color: #999;
            }
            .bg-denounce {
                background-color: #c82061 !important;
            }
            .bg-sugestion {
                background-color: #1155cc;
            }
            .bg-complaint {
                background-color: #681133;
            }
            .bg-comp-book { 
                background-color: #f39c12;
                color: #fff;
            }
            .bg-intern-recl {
                background-color: #6eb63e;
            }
            .bg-extern-recl {
                background-color: #4e802c;
            }
            .small-box .inner {
                color: #fff;
            }
            .response-area textarea {                
            }
            .sf-toolbar {
                display: none !important;
            }
            .app-footer {
                position: fixed;
                left: 0;
                bottom: 0;
                width: 100%;
                background-color: #333;
                color: #fff;
                text-align: center;
                padding: 10px;
            }
        </style>
    </head>

    <body class="skin-blue sidebar-mini pace-done fixed" ng-app="app">
        {% block body %}
            <div class="wrapper">
            </div>
        {% endblock %}

        {% javascripts
            '@BackendBundle/Resources/public/js/jQuery-2.2.0.min.js'
            '@BackendBundle/Resources/public/js/bootstrap.min.js'
            '@BackendBundle/Resources/public/js/fastclick.js'
            '@BackendBundle/Resources/public/js/app.min.js'
            '@BackendBundle/Resources/public/js/jquery.sparkline.min.js'
            '@BackendBundle/Resources/public/js/jquery-jvectormap-1.2.2.min.js'
            '@BackendBundle/Resources/public/js/jquery-jvectormap-world-mill-en.js'
            '@BackendBundle/Resources/public/js/jquery.slimscroll.min.js'
            '@BackendBundle/Resources/public/js/Chart.min.js'
            '@BackendBundle/Resources/public/js/demo.js'
            '@BackendBundle/Resources/public/js/pace.min.js'
            '@BackendBundle/Resources/public/js/moment.min.js'
            '@BackendBundle/Resources/public/js/lodash.js'
            '@BackendBundle/Resources/public/js/icheck.min.js'
            '@BackendBundle/Resources/public/js/notify.js'
            '@BackendBundle/Resources/public/js/highcharts/exporting.js'
            '@BackendBundle/Resources/public/js/highcharts/highcharts.js'
            '@BackendBundle/Resources/public/js/highcharts/series-label.js'
            '@BackendBundle/Resources/public/lib/editor/trumbowyg.min.js'
            '@BackendBundle/Resources/public/js/angular.min.js'

            '@BackendBundle/Resources/public/js/app/app.js'
            '@BackendBundle/Resources/public/js/app/admin/service.js'
            '@BackendBundle/Resources/public/js/app/admin/controller.js'
            '@BackendBundle/Resources/public/js/app/dashboard/controller.js'
            '@BackendBundle/Resources/public/js/app/dashboard/statistics.js'
            '@BackendBundle/Resources/public/js/app/statistics/service.js'

            '@BackendBundle/Resources/public/js/app/sugestion/controller.js'
            '@BackendBundle/Resources/public/js/app/sugestion/acompanhamento.js'
            '@BackendBundle/Resources/public/js/app/sugestion/tratamento.js'
            '@BackendBundle/Resources/public/js/app/sugestion/service.js'

            '@BackendBundle/Resources/public/js/app/compbook/controller.js'
            '@BackendBundle/Resources/public/js/app/compbook/service.js'

            '@BackendBundle/Resources/public/js/app/complaint/controller.js'
            '@BackendBundle/Resources/public/js/app/complaint/service.js'
            '@BackendBundle/Resources/public/js/app/complaint/acompanhamento.js'
            '@BackendBundle/Resources/public/js/app/complaint/tratamento.js'

            '@BackendBundle/Resources/public/js/app/ireclamation/controller.js'
        %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}

        {% block jsCode %}
            <div class="wrapper">
            </div>
        {% endblock %}

        <div class="app-footer">
            <div class="pull-right">
            <p>POWERED BY ARFA - 2018</p>
            </div>
        </div>
    </body>
</html>