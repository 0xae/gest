{% extends 'BackendBundle:Default:layout.html.twig' %}

{% block content %}

<div class="row">
    <div class="col-xs-12">
        <div class="box">
            <div class="box-header">
                <h3 class="box-title">Complaint #{{ entity.id }}</h3>
                <p>
                    <a href="{{ path('administration_Complaint') }}">
                        <span class="glyphicon glyphicon-back"></span> Voltar
                    </a>
                </p>
            </div>
            <!-- /.box-header -->

            <div class="box-body no-padding">
                <table class="table table-hover table-striped">
                    <tbody>
                        <tr>
                            <th>Id</th>
                            <td>{{ entity.id }}</td>
                        </tr>
                        <tr>
                            <th>Name</th>
                            <td>{{ entity.name }}</td>
                        </tr>
                        <tr>
                            <th>Address</th>
                            <td>{{ entity.address }}</td>
                        </tr>
                        <tr>
                            <th>Locality</th>
                            <td>{{ entity.locality }}</td>
                        </tr>
                        <tr>
                            <th>Phone</th>
                            <td>{{ entity.phone }}</td>
                        </tr>
                        <tr>
                            <th>Email</th>
                            <td>{{ entity.email }}</td>
                        </tr>
                        <tr>
                            <th>Type</th>
                            <td>{{ entity.type }}</td>
                        </tr>
                        <tr>
                            <th>Opname</th>
                            <td>{{ entity.opName }}</td>
                        </tr>
                        <tr>
                            <th>Opaddress</th>
                            <td>{{ entity.opAddress }}</td>
                        </tr>
                        <tr>
                            <th>Oplocality</th>
                            <td>{{ entity.opLocality }}</td>
                        </tr>
                        <tr>
                            <th>Opphone</th>
                            <td>{{ entity.opPhone }}</td>
                        </tr>
                        <tr>
                            <th>Opemail</th>
                            <td>{{ entity.opEmail }}</td>
                        </tr>
                        <tr>
                            <th>Factdate</th>
                            <td>{{ entity.factDate|date('Y-m-d H:i:s') }}</td>
                        </tr>
                        <tr>
                            <th>Factannex</th>
                            <td>{{ entity.factAnnex }}</td>
                        </tr>
                        <tr>
                            <th>Factdetail</th>
                            <td>{{ entity.factDetail }}</td>
                        </tr>
                        <tr>
                            <th>Hasproduct</th>
                            <td>{{ entity.hasProduct }}</td>
                        </tr>
                        <tr>
                            <th>Annex</th>
                            <td>{{ entity.annex }}</td>
                        </tr>
                        <tr>
                            <th>Annextype</th>
                            <td>{{ entity.annexType }}</td>
                        </tr>
                        <tr>
                            <th>Createdat</th>
                            <td>{{ entity.createdAt|date('Y-m-d H:i:s') }}</td>
                        </tr>
                        <tr>
                            <th>Createdby</th>
                            <td>{{ entity.createdBy }}</td>
                        </tr>
                    </tbody>
                </table>
            </div>
            <!-- /.box-body -->
        </div>
        <!-- /.box -->
    </div>
</div>

{% endblock %}

