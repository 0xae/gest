<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Evaluation" table="evaluation">
    <indexes>
      <index name="fk_evaluation_lesson1_idx" columns="lesson_id"/>
      <index name="fk_evaluation_users1_idx" columns="student_id"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="id_UNIQUE" columns="id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="grade" type="integer" column="grade" nullable="true"/>
    <field name="documentDir" type="string" column="document_dir" length="100" nullable="true"/>
    <field name="title" type="string" column="title" length="45" nullable="true"/>
    <field name="startProjectDate" type="datetime" column="start_project_date" nullable="true"/>
    <field name="endProjectDate" type="datetime" column="end_project_date" nullable="true"/>
    <many-to-one field="lesson" target-entity="Lesson">
      <join-columns>
        <join-column name="lesson_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="student" target-entity="User">
      <join-columns>
        <join-column name="student_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
