<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CourseHasDiscipline" table="course_has_discipline">
    <indexes>
      <index name="fk_courses_has_modules_modules1_idx" columns="discipline_id"/>
      <index name="fk_courses_has_modules_courses1_idx" columns="course_id"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="id_UNIQUE" columns="id"/>
      <unique-constraint name="courses_discipline_UNIQUE" columns="course_id,discipline_id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="createdAt" type="datetime" column="created_at" nullable="false"/>
    <field name="hourWorkload" type="integer" column="hour_workload" nullable="false"/>
    <field name="createdBy" type="integer" column="created_by" nullable="false"/>
    <many-to-one field="course" target-entity="Course">
      <join-columns>
        <join-column name="course_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="discipline" target-entity="Discipline">
      <join-columns>
        <join-column name="discipline_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
