<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="UserHasKlass" table="user_has_klass">
    <indexes>
      <index name="fk_users_has_class_class1_idx" columns="klass_id"/>
      <index name="fk_users_has_class_users1_idx" columns="user_id"/>
      <index name="fk_users_has_class_discipline1_idx" columns="discipline_id"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="id_UNIQUE" columns="id"/>
      <unique-constraint name="users_class_discipline_UNIQUE" columns="user_id,klass_id,discipline_id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="createdAt" type="datetime" column="created_at" nullable="false"/>
    <field name="createdBy" type="integer" column="created_by" nullable="false"/>
    <many-to-one field="klass" target-entity="Klass">
      <join-columns>
        <join-column name="klass_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="discipline" target-entity="Discipline">
      <join-columns>
        <join-column name="discipline_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="user" target-entity="User">
      <join-columns>
        <join-column name="user_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
