<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Enrollment" table="enrollment">
    <indexes>
      <index name="fk_courses_has_student_student1_idx" columns="user_id"/>
      <index name="fk_courses_has_student_courses1_idx" columns="course_id"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="id_UNIQUE" columns="id"/>
      <unique-constraint name="courses_users_UNIQUE" columns="course_id,user_id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="createdAt" type="datetime" column="created_at" nullable="false"/>
    <field name="schoolYear" type="date" column="school_year" nullable="true"/>
    <field name="createdBy" type="integer" column="created_by" nullable="false"/>
    <many-to-one field="course" target-entity="Course">
      <join-columns>
        <join-column name="course_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="user" target-entity="User">
      <join-columns>
        <join-column name="user_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
